{
  "name": "ngcarparking-lot",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --prod --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint ngx-admin-demo --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0-rc.0",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "~7.0.0-rc.0",
    "@angular/compiler": "~7.0.0-rc.0",
    "@angular/core": "~7.0.0-rc.0",
    "@angular/forms": "~7.0.0-rc.0",
    "@angular/http": "~7.0.0-rc.0",
    "@angular/platform-browser": "~7.0.0-rc.0",
    "@angular/platform-browser-dynamic": "~7.0.0-rc.0",
    "@angular/router": "~7.0.0-rc.0",
    "@nebular/auth": "^2.0.1",
    "@nebular/security": "^2.0.1",
    "@nebular/theme": "^2.0.1",
    "@ng-bootstrap/ng-bootstrap": "^3.3.0",
    "core-js": "^2.5.4",
    "ngx-tinymce": "^2.1.1",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.3.3",
    "tinymce": "4.5.7",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.9.0-rc.1",
    "@angular/cli": "~7.0.0-rc.1",
    "@angular/compiler-cli": "~7.0.0-rc.0",
    "@angular/language-service": "~7.0.0-rc.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
